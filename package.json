{
  "name": "image-carousel",
  "version": "1.0.0",
  "description": "> Create image carousel component of a yelp-like webpage. Serving API and relative components to a proxy server.",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "build-dev": "webpack -d --watch",
    "build": "webpack --config webpack.prod.js",
    "start": "nodemon ./server/index.js",
    "database": "mysql -u root < database/schema.sql",
    "seed": "psql -p5432 -d yelp < database/postgreSQL/schema.sql",
    "psql": "psql -d yelp",
    "cass": "cqlsh",
    "seed-cass": "cqlsh -f 'schema.cql'"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "transform": {
      ".*": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dev-Stars/image-carousel.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dev-Stars/image-carousel/issues"
  },
  "homepage": "https://github.com/Dev-Stars/image-carousel#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "mysql": "^2.16.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "request": "^2.87.0",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0"
  },
  "devDependencies": {
    "babel-jest": "^23.4.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^23.4.1",
    "jest-css-modules": "^1.1.0",
    "jest-enzyme": "^6.0.2",
    "sinon": "^6.1.4",
    "style-loader": "^0.21.0",
    "webpack-merge": "^4.1.3"
  }
}
